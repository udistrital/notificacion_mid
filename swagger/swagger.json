                    "colas"
                ],
                "description": "Borra la cola",
                "operationId": "ColasController.BorrarCola",
                "parameters": [
                    {
                        "in": "path",
                        "name": "cola",
                        "description": "Nombre de la cola a borrar",
                        "required": true,
                        "type": "string",
                        "pattern": "[^.]$"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} Cola eliminada"
                    },
                    "502": {
                        "description": "Error en borrado de cola"
                    }
                }
            }
